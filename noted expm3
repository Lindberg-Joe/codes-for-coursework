#define MAX 50
/* 建立保留字表 */
#define MAX_KEY_NUMBER 20  /*关键字的数量*/
#define KEY_WORD_END "waiting for your expanding"  /*关键字结束标记*/
char *KeyWordTable[MAX_KEY_NUMBER]={ "if", "then", "else", "while", "begin", "end" , "int" , "for", "do" , KEY_WORD_END};
/* 查保留字表，判断是否为关键字 */

char TOKEN[MAX];
int stacks[MAX];	//状态栈
int s=0;			//index of stacks状态栈指针
