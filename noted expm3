	strcpy(results[idxofres].name,res); 
	strcpy(temp,valuesta[idxofv-2]);
	strcat(temp,"-");
	strcat(temp,valuesta[idxofv-1]);
	strcpy(results[idxofres].val,temp); 
	strcpy(valuesta[idxofv-2],res);
	idxofres++;
	idxofv-=1; 
}

